{
  "name": "react-typescript-example",
  "version": "1.0.0",
  "description": "The project using react+typescript+webpack",
  "main": "./app.js",
  "scripts": {
    "clean": "rm -r build",
    "build": "webpack --config webpack.config.js",
    "build:dev": "webpack --config webpack.config.js --develop --hot",
    "start": "NODE_ENV=production electron ./",
    "start:dev": "NODE_ENV=development HOT=1 electron ./ --debug",
    "start-api": "babel-node api/index.js",
    "serve": "node devServer.js --hot --develop --verbose",
    "release": "node release.js",
    "watch": "concurrently --kill-others \"npm run start:dev\" \"npm run serve\"",
    "watch-api": "pm2 start api/index.js --watch --interpreter babel-node",
    "test": "karma start",
    "lint": "tslint -c tslint.json "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sue71/react-typescript-example.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sue71/react-typescript-example/issues"
  },
  "homepage": "https://github.com/sue71/react-typescript-example#readme",
